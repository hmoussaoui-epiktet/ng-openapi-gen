{
	"openapi": "3.0.0",
	"info": {
		"title": "Test default factories",
		"version": "1.0"
	},
	"paths": {
		"/users": {
			"get": {
				"responses": {
					"200": {
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/User"
								}
							}
						}
					}
				}
			}
		},
		"/products": {
			"get": {
				"responses": {
					"200": {
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Product"
								}
							}
						}
					}
				}
			}
		}
	},
	"components": {
		"schemas": {
			"User": {
				"type": "object",
				"properties": {
					"id": {
						"type": "integer"
					},
					"name": {
						"type": "string"
					},
					"email": {
						"type": "string"
					},
					"age": {
						"type": "integer"
					},
					"isActive": {
						"type": "boolean"
					},
					"tags": {
						"type": "array",
						"items": {
							"type": "string"
						}
					},
					"metadata": {
						"type": "object"
					}
				},
				"required": ["id", "name", "email"]
			},
			"Product": {
				"type": "object",
				"properties": {
					"sku": {
						"type": "string"
					},
					"price": {
						"type": "number"
					},
					"inStock": {
						"type": "boolean"
					},
					"category": {
						"$ref": "#/components/schemas/Category"
					}
				},
				"required": ["sku", "price"]
			},
			"Category": {
				"type": "object",
				"properties": {
					"id": {
						"type": "integer"
					},
					"name": {
						"type": "string"
					}
				},
				"required": ["id", "name"]
			},
			"Status": {
				"type": "string",
				"enum": ["active", "inactive", "pending"]
			},
			"NullableField": {
				"type": "object",
				"properties": {
					"value": {
						"type": "string",
						"nullable": true
					}
				}
			}
		}
	}
}
